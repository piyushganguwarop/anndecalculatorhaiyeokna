# --------------------------
# Discord Bot Configuration
# --------------------------

# Required secrets
DISCORD_TOKEN=YOUR_BOT_TOKEN_HERE
GUILD_ID=YOUR_DISCORD_SERVER_ID
CHANNEL_ID=YOUR_CHANNEL_ID

# Only count webhook messages (1 = only webhook, 0 = count everything)
ONLY_WEBHOOK=1

# France = 1 (winter) or 2 (summer). Set 0 for UTC.
TZ_OFFSET_HOURS=1

# Regex patterns for egg types
EGG_PARADISE_PATTERN=(?i)paradise
EGG_SAFARI_PATTERN=(?i)safari

# Global multiplier for missing logs
LOSS_MULT=1.0
